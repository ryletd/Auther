{
  "manifest_version": 3,

  "name": "Auther",
  "description": "Free 2FA Authenticator app, Chrome extension alternative to Google Authenticator.",
  "version": "1.2",

  "icons": {
    "512": "favicon.png"
  },

  "action": {
    "default_title": "Auther",
    "default_popup": "index.html"
  },

  "options_page": "options.html",

  "permissions": ["storage"]
}
